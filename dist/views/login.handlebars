<body>
  
  <link rel="stylesheet" href="css/signInPAge.css">
  <div class="outerMainDiv container">
        <div class="innerMainDiv container">
            <h2 class="signInHeader">Sign In</h2>
         
            <div class="emailDiv">
                <label for="email" class="emailLab">
                  Email address
                </label>
              
                  <input id="email" name="email" type="email" autocomplete="email" class="form-control">
            
              </div>
              <div class="passwordDiv">
                <label for="password" class="">
                  Password
                </label>
                
                  <input id="password" name="password" type="password" autocomplete="password" class="form-control">
                

                <div class="row">

                  <button type='button' class="btn btn-secondary d-flex justify-content-start justify-content-around" style="width: 400px;" id="signIn">Sign In</button>
                  <button type='button' class="btn btn-secondary  d-flex justify-content-around justify-content-end" style="width: 400px;" id="createAccount">Create an Account</button>
                </div>
              </div>  

        </div>
  </div>   
   <script src='/js/loginFrontEnd.js'></script>
   <script src='/js/navBar.js'></script>
   
</body>